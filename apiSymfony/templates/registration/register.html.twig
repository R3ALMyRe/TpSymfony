{% extends 'base.html.twig' %}

{% block title %}Registration{% endblock %}

{% block body %}
    <h1>Register</h1>

    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.name) }}
        {{ form_row(form.phoneNumber) }}
        <button type="submit">Register</button>
    {{ form_end(form) }}
{% endblock %}
